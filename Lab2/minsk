<script type="text/javascript">/* <![CDATA[ */
(function (nameAttr) {
	$(nameAttr).on({
	'mouseenter': function () {
		$('[data-day-lite="'+$(this).data('day-lite')+'"]').addClass('hover');
	},
	'mouseleave': function () {
	$('[data-day-lite="'+$(this).data('day-lite')+'"]').removeClass('hover');
	},
	'click': function () {
		var oForm = $("#external-search");
		var oLink = $(this).data('day-lite');
		oForm.data('oAction', oForm.attr('action'));
		oForm.attr('action','#day-'+$(this).data('day-lite'));
		$('a[href="#tab-future"]').trigger('click');
		oForm.submit();
		$(window).scrollTop($('[name="day-'+oLink+'"]').offset().top); //for opera 12
		oForm.attr('action', "");
		oForm.attr('action', oForm.data('oAction'));
	}
	});
})('[data-day-lite]'); 
/* ]]> */</script>
